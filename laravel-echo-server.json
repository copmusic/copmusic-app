{
    "authHost": "http://127.0.0.1",
    "authEndpoint": "/broadcasting/auth",
    "clients": [],
    "database": "redis",
    "databaseConfig": {
        "redis": {
            "host": "lws-redis",
            "port": "6379",
            "options": {
                "db": "0"
            }
        },
        "sqlite": {
            "databasePath": "/app/database/laravel-echo-server.sqlite"
        },
        "publishPresence": true
    },
    "devMode": false,
    "host": "lws-websockets",
    "port": 6001,
    "protocol": "http",
    "subscribers": {
        "http": true,
        "redis": true
    },
    "apiOriginAllow": {
        "allowCors": true,
        "allowOrigin": "http://127.0.0.1",
        "allowMethods": "GET, OPTION, POST",
        "allowHeaders": "Accept, Authorization, Content-Type, Origin, X-Auth-Token, X-CSRF-TOKEN, X-Requested-With, X-Socket-Id"
    }
}
